{
  "version": 2,
  "buildCommand": "pwd; ls -la; npm --prefix ./Desktop/Rasim\\ otomotiv-PANEL/frontend ci && npm --prefix ./Desktop/Rasim\\ otomotiv-PANEL/frontend run build",
  "devCommand": "npm --prefix ./Desktop/Rasim\\ otomotiv-PANEL/frontend run dev",
  "installCommand": "npm --prefix ./Desktop/Rasim\\ otomotiv-PANEL/frontend ci",
  "framework": "vite",
  "outputDirectory": "Desktop/Rasim otomotiv-PANEL/frontend/dist",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}