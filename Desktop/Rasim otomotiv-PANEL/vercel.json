{
  "version": 2,
  "buildCommand": "pwd && ls -la && find . -name 'frontend' -type d && cd \"Desktop/Rasim otomotiv-PANEL/frontend\" && npm ci && npm run build",
  "devCommand": "cd \"Desktop/Rasim otomotiv-PANEL/frontend\" && npm run dev",
  "installCommand": "cd \"Desktop/Rasim otomotiv-PANEL/frontend\" && npm ci",
  "framework": "vite",
  "outputDirectory": "Desktop/Rasim otomotiv-PANEL/frontend/dist",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
